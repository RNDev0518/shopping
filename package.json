{
  "name": "eurochat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "cd node_modules/react-native/scripts && ./ios-install-third-party.sh && cd ../../../ && cd node_modules/react-native/third-party/glog-0.3.5/ && ../../scripts/ios-configure-glog.sh && cd ../../../../",
    "start": "react-native start",
    "run:ios": "react-native run-ios",
    "run:android": "react-native run-android",
    "clean:apk": "rm -rf ./android/app/build/outputs/debug/app-debug.apk",
    "prerun:android": "rm -rf ./android/app/src/main/assets/index.android.bundle && yarn clean:apk",
    "run:android:prod": "react-native run-android",
    "prerun:android:prod": "yarn bundle:android && yarn clean:apk",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle  --assets-dest android/app/src/main/res/",
    "bundle:ios": "react-native bundle --entry-file=index.js --bundle-output=./ios/eurochat/main.jsbundle --dev=false --platform=ios --assets-dest=./ios"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "buffer": "^5.2.1",
    "frisbee": "^2.0.5",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "parse": "^2.1.0",
    "react": "16.5.0",
    "react-native": "0.57.0",
    "react-native-action-button": "^2.8.5",
    "react-native-actionsheet": "^2.4.2",
    "react-native-country-picker-modal": "^0.6.2",
    "react-native-dialog": "^5.4.0",
    "react-native-elements": "1.0.0-beta5",
    "react-native-facebook-login": "^1.6.1",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-form": "^2.1.3",
    "react-native-geocoder": "^0.5.0",
    "react-native-google-signin": "^1.0.0-rc5",
    "react-native-hr-plus": "^1.0.6",
    "react-native-icon-badge": "^1.1.3",
    "react-native-image-crop-picker": "^0.21.1",
    "react-native-image-picker": "^0.27.1",
    "react-native-image-resizer": "^1.0.0",
    "react-native-img-cache": "^1.5.3",
    "react-native-lightbox": "^0.8.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-maps": "^0.22.0",
    "react-native-maps-directions": "^1.6.0",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-material-textfield": "^0.12.0",
    "react-native-modal": "^6.5.0",
    "react-native-phone-auth-component": "^0.1.4",
    "react-native-picker": "^4.3.7",
    "react-native-root-toast": "^3.0.1",
    "react-native-segmented-control-tab": "^3.3.1",
    "react-native-sensors": "^4.0.0",
    "react-native-switch-pro": "^1.0.1-beta",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.14.0",
    "react-redux": "^5.0.7",
    "realm": "2.16.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.4.9",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "@types/react": "^16.4.14",
    "@types/react-native": "^0.57.0",
    "@types/react-native-material-textfield": "^0.12.2",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.45.3",
    "react-test-renderer": "16.5.0"
  },
  "jest": {
    "preset": "react-native"
  }
